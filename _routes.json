{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/assets/*"],
  "routes": [
    {
      "src": "/models/(.*)",
      "dest": "https://nihaalnazeer.com/models/$1"
    },
    {
      "src": "/hdris/(.*)",
      "dest": "https://nihaalnazeer.com/hdris/$1"
    },
    {
      "src": "/backgroundvideos/(.*)",
      "dest": "https://nihaalnazeer.com/backgroundvideos/$1"
    },
    {
      "src": "/buttonoverlays/(.*)",
      "dest": "https://nihaalnazeer.com/buttonoverlays/$1"
    },
    {
      "src": "/interactiveanimations/(.*)",
      "dest": "https://nihaalnazeer.com/interactiveanimations/$1"
    },
    {
      "src": "/interactiveimages/(.*)",
      "dest": "https://nihaalnazeer.com/interactiveimages/$1"
    },
    {
      "src": "/videos/(.*)",
      "dest": "https://nihaalnazeer.com/videos/$1"
    }
  ]
} 